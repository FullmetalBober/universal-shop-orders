{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://universal-shop-orders-api.vercel.app/api/:path*"
    },
    {
      "source": "/api/:path*/",
      "destination": "https://universal-shop-orders-api.vercel.app/api/:path*/"
    },
    { "source": "/(.*)", "destination": "/" }
  ]
}
